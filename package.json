{
  "name": "circuits-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "prettify": "prettier --check './**/*.{ts,tsx}'",
    "prettify:write": "prettier --write './**/*.{ts,tsx}'",
    "convert-icons": "npx @svgr/cli --typescript --no-prettier --icon --out-dir ./src/components/icons ./public/icons"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "crypto": false,
    "@microsoft/typescript-etw": false,
    "source-map-support": false,
    "inspector": false
  },
  "dependencies": {
    "@solana/wallet-adapter-react": "^0.15.32",
    "@solana/web3.js": "^1.78.0",
    "@types/node": "20.4.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "immer": "^10.0.2",
    "next": "13.4.9",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.9",
    "@svgr/cli": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.0"
  }
}
