{
	"semi": true,
	"trailingComma": "es5",
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"bracketSameLine": false,
	"importOrder": [
		"^@/components/(.*)$",
		"^@/hooks/(.*)$",
		"^@/utils/(.*)$",
		"^@/constants/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": [
		"@trivago/prettier-plugin-sort-imports"
	]
}